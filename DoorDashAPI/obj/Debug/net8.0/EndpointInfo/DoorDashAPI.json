            "minLength": 0,
            "type": "string"
          },
          "city": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "state": {
            "maxLength": 25,
            "minLength": 0,
            "type": "string"
          },
          "openingHour": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "format": "date-span"
          },
          "closingHour": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "format": "date-span"
          },
          "isVeg": {
            "type": "boolean"
          },
          "isOpen": {
            "type": "boolean",
            "nullable": true
          },
          "averageRating": {
            "maximum": 5,
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "costForTwo": {
            "maxLength": 35,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "restaurantImage": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "restaurantLink": {
            "maxLength": 255,
            "minLength": 0,
